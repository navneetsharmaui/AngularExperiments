{
	"name": "angular-experiments",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "npm run postinstall.web && ng serve web-angular-app",
		"build": "npm run postinstall.web && ng build web-angular-app",
		"test": "npm run postinstall.web && ng test",
		"lint": "nx workspace-lint && ng lint",
		"e2e": "npm run postinstall.web && ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"update": "ng update @nrwl/workspace",
		"update:check": "ng update",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"clean": "npx rimraf hooks node_modules package-lock.json && npm i",
		"start.api": "ng serve api",
		"start.web.react-app": "ng serve react-app",
		"start.web.angular-app": "npm run postinstall.web && ng serve web-angular-app",
		"postinstall": "electron-rebuild install-app-deps",
		"postinstall.electron": "node tools/electron/postinstall",
		"postinstall.web": "node tools/web/postinstall",
		"build.electron.angular-app": "npm run prepare.electron.angular-app && ng build electron-angular-app --prod --base-href ./",
		"build.electron.angular-app.local": "npm run build.electron.angular-app && electron dist/apps/electron-angular-app",
		"build.electron.angular-app.linux": "npm run build.electron.angular-app && cd dist/apps/electron-angular-app && npx electron-builder build --linux",
		"build.electron.angular-app.windows": "npm run build.electron.angular-app && cd dist/apps/electron-angular-app && npx electron-builder build --windows",
		"build.electron.angular-app.mac": "npm run build.electron.angular-app && cd dist/apps/electron-angular-app && npx electron-builder build --mac",
		"prepare.electron.angular-app": "npm run postinstall.electron && tsc -p apps/electron-angular-app/tsconfig.json",
		"serve.electron.angular-app.target": "ng serve electron-angular-app",
		"serve.electron.angular-app": "wait-on http-get://localhost:4200/ && electron apps/electron-angular-app/src --serve",
		"start.electron.angular-app": "npm run prepare.electron.angular-app && npm-run-all -p serve.electron.angular-app.target serve.electron.angular-app",
		"build.web.elements": "ng build web-elements --prod --output-hashing=none --single-bundle=true --keep-polyfills=true",
		"preview.web.elements": "http-server dist/ngelements"
	},
	"private": true,
	"dependencies": {
		"@nestjs/common": "^6.2.4",
		"@nestjs/core": "^6.2.4",
		"@nestjs/platform-express": "^6.2.4",
		"@zeit/next-css": "1.0.1",
		"@zeit/next-less": "1.0.1",
		"@zeit/next-sass": "1.0.1",
		"@zeit/next-stylus": "1.0.1",
		"next": "11.1.3",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"reflect-metadata": "^0.1.12",
		"@angular/animations": "^8.2.0",
		"@angular/common": "^8.2.0",
		"@angular/compiler": "^8.2.0",
		"@angular/core": "^8.2.0",
		"@angular/forms": "^8.2.0",
		"@angular/platform-browser": "^8.2.0",
		"@angular/platform-browser-dynamic": "^8.2.0",
		"@angular/router": "^8.2.0",
		"core-js": "^3.1.4",
		"rxjs": "~6.4.0",
		"zone.js": "~0.9.0",
		"@exps/scss": "file:libs/scss",
		"@ngx-translate/core": "~11.0.0",
		"@ngx-translate/http-loader": "~4.0.0",
		"react-router-dom": "5.0.1",
		"@angular/elements": "^8.2.0",
		"@webcomponents/webcomponentsjs": "^2.2.7"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.803.3",
		"@angular/cli": "8.3.3",
		"@angular/compiler-cli": "^8.2.0",
		"@angular/language-service": "^8.2.0",
		"@babel/preset-react": "7.0.0",
		"@nestjs/schematics": "^6.3.0",
		"@nestjs/testing": "^6.2.4",
		"@nrwl/angular": "~8.4.0",
		"@nrwl/cypress": "8.6.0",
		"@nrwl/express": "^8.6.0",
		"@nrwl/jest": "8.6.0",
		"@nrwl/nest": "^8.6.0",
		"@nrwl/next": "^8.6.0",
		"@nrwl/node": "^8.6.0",
		"@nrwl/react": "^8.6.0",
		"@nrwl/web": "^8.6.0",
		"@nrwl/workspace": "8.6.0",
		"@nstudio/angular": "8.0.10",
		"@nstudio/electron": "8.0.10",
		"@nstudio/electron-angular": "8.0.10",
		"@nstudio/schematics": "^8.0.10",
		"@nstudio/web": "8.0.10",
		"@nstudio/web-angular": "8.0.10",
		"@nstudio/xplat": "^8.0.10",
		"@testing-library/react": "8.0.5",
